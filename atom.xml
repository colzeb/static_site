&lt;&#x2F;span&gt;&lt;span&gt;    &lt;&#x2F;span&gt;&lt;span style=&quot;color:#ffb62c;&quot;&gt;String&lt;&#x2F;span&gt;&lt;span&gt; temp &lt;&#x2F;span&gt;&lt;span style=&quot;color:#39adb5;&quot;&gt;=&lt;&#x2F;span&gt;&lt;span&gt; names&lt;&#x2F;span&gt;&lt;span style=&quot;color:#39adb5;&quot;&gt;.&lt;&#x2F;span&gt;&lt;span style=&quot;color:#6182b8;&quot;&gt;get&lt;&#x2F;span&gt;&lt;span style=&quot;color:#39adb5;&quot;&gt;(&lt;&#x2F;span&gt;&lt;span style=&quot;color:#f76d47;&quot;&gt;1&lt;&#x2F;span&gt;&lt;span style=&quot;color:#39adb5;&quot;&gt;);
